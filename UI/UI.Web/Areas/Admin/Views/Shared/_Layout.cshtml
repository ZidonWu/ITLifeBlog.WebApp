<!DOCTYPE html>

<html>
<head>
    <title>ITLifeBlog.WebApp.Admin</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:300,400' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700,900' rel='stylesheet' type='text/css'>
    <!-- CSS Libs -->
    <link rel="stylesheet" type="text/css" href="~/Src/Admin/lib/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Src/Admin/lib/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Src/Admin/lib/css/animate.min.css">
    <link rel="stylesheet" type="text/css" href="~/Src/Admin/lib/css/bootstrap-switch.min.css">
    <link rel="stylesheet" type="text/css" href="~/Src/Admin/lib/css/checkbox3.min.css">
    <link rel="stylesheet" type="text/css" href="~/Src/Admin/lib/css/jquery.dataTables.min.css">
    <link rel="stylesheet" type="text/css" href="~/Src/Admin/lib/css/dataTables.bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Src/Admin/lib/css/select2.min.css">
    <!-- CSS App -->
    <link rel="stylesheet" type="text/css" href="~/Src/Admin/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Src/Admin/css/themes/flat-blue.css">
</head>
<body class="flat-blue">
    <div class="app-container">
        <div class="row content-container">
            @Html.Partial("_HeaderPartial")
            @Html.Partial("_SidebarPartial")  
            <!-- Main Content -->
            @RenderBody()
        </div>
        <footer class="app-footer">
            <div class="wrapper">
                <span class="pull-right"><a href="#"><i class="fa fa-long-arrow-up"></i></a></span> © @DateTime.Now.Year Copyright. 
            </div>
        </footer>
    </div>

    <!-- Javascript Libs -->
    <script type="text/javascript" src="~/Src/Admin/lib/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/Src/Admin/lib/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Src/Admin/lib/js/Chart.min.js"></script>
    <script type="text/javascript" src="~/Src/Admin/lib/js/bootstrap-switch.min.js"></script>
    <script type="text/javascript" src="~/Src/Admin/lib/js/jquery.matchHeight-min.js"></script>
    <script type="text/javascript" src="~/Src/Admin/lib/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Src/Admin/lib/js/dataTables.bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Src/Admin/lib/js/select2.full.min.js"></script>
    <script type="text/javascript" src="~/Src/Admin/lib/js/ace/ace.js"></script>
    <script type="text/javascript" src="~/Src/Admin/lib/js/ace/mode-html.js"></script>
    <script type="text/javascript" src="~/Src/Admin/lib/js/ace/theme-github.js"></script>
    <!-- Javascript -->
    <script type="text/javascript" src="~/Src/Admin/js/app.js"></script>
    <script type="text/javascript" src="~/Src/Admin/js/index.js"></script>
</body>
</html>
